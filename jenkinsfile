pipeline {
    agent any {


    environment {

        SCANNER_HOME=tool 'sonar-mvn'
    }

    stages {


        stage("validate") {

            steps {

                sh "mvn validate"
            }
        }

        stage("compile") {

            steps {

                sh "mvn clean compile"
            }
        }

        stage("Test cases") {

            steps {

                sh "mvn test"
            
            }
        }

        stage("build"){

            steps {
                
                sh "mvn clean install"
            }
        }




    }
    }
}